msgctxt "Addons::name::cp_advanced_password"
msgid "Cart-Power: Advanced password"
msgstr "Cart-Power: Улучшенный пароль"

msgctxt "Addons::description::cp_advanced_password"
msgid "Allows you to select the minimum length and additional password settings. Developed by <a target=\"_blank\" href=\"http://cart-power.com/\">Cart-Power</a>"
msgstr "Позволяет выбрать минимальную длину и дополнительные настройки пароля. Разработано <a target=\"_blank\" href=\"http://cart-power.com/\">Cart-Power</a>"

msgctxt "SettingsOptions::cp_advanced_password::pass_min_length"
msgid "The minimum length for the password"
msgstr "Минимальная длина пароля"

msgctxt "SettingsOptions::cp_advanced_password::pass_additional_settings"
msgid "Additional password settings"
msgstr "Дополнительные настройки пароля"

msgctxt "SettingsOptions::cp_advanced_password::use_in_admin_panel"
msgid "Apply rules for the admin panel"
msgstr "Применять правила для панели администратора"

msgctxt "Languages::cp_advanced_password.use_pass_numbers"
msgid "The password must contain numbers"
msgstr "Пароль должен содержать цифры"

msgctxt "Languages::cp_advanced_password.use_pass_upper"
msgid "The password must contain uppercase characters"
msgstr "Пароль должен содержать символы в верхнем регистре"

msgctxt "Languages::cp_advanced_password.use_pass_sumbols"
msgid "The password must contain special characters"
msgstr "Пароль должен содержать спец. символы"

msgctxt "Languages::cp_advanced_password.error_pass_length"
msgid "The minimum password length must be [pass_min_length] symbols"
msgstr "Минимальная длина пароля [pass_min_length] символов"  